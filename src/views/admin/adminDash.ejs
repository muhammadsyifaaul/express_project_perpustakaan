<a href="/createData" class="btn btn-success badge mt-4">Add Data</a>

<div class="d-flex gap-3">
    <div class="mt-4">
        <h5>Book Data</h5>
        <% authors.forEach(author => { %> 
            <ul>
                <% author.books.forEach(book => { %> 
                    <li><a href="/details/book/<%- book._id %>"><%- book.title %></a></li> 
                <% }) %>
            </ul>
        <% }) %>
    </div>
    <div class="mt-4">
        <h5>Library Data</h5>
        <ul>
            <% library.forEach(lib => { %>
                <li><a href="/details/library/<%- lib._id %>"><%- lib.name %></a></li>
            <% }) %>
        </ul>
    </div>
    <div class="mt-4">
        <h5>Author Data</h5>
        <ul>
            <% authors.forEach(author => { %> 
                 <li><a href="/details/author/<%- author._id %>"><%- author.name %></a></li> 
            <% }) %>
        </ul>
    </div>
</div>